package mx.vw.swf.banner.api.exception.module.b;

import mx.vw.swf.banner.api.exception.*;

/**
 * Esta clase se encarga de manejar todas las excepciones generadas en el Sistema , especificamente en el modulo A y propagarlas a nivel superior.
 * 
 * @author Rafael Miranda Blumenkron
 * @version 1.0
 * @created 10-abr-2014 11:04:31 a.m.
 */
public class BANNERModuleBException extends BANNERException {
    private static final long serialVersionUID = 4508461886822251902L;
    /**
     * Nombre del modulo.
     */
    private static final String NOM_MODULO = "MOD_A";

    public BANNERModuleBException() {
        super(NOM_MODULO, "00");
    }

    /**
     * Constructor que se encarga de desplegar el mensaje con el codigo recibido para el nombre de modulo predeterminados.
     * 
     * @param errorCode
     *            Codigo de error que debe desplegarse.
     */
    public BANNERModuleBException(String errorCode) {
        super(NOM_MODULO, errorCode);
    }
}// end