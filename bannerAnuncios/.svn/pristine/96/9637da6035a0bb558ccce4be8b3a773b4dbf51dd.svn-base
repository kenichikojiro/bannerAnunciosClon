	<subDataset name="menuOptions" uuid="91e70e86-1998-4975-b2de-ef231c42473e">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="${classPrefix}_DEV"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w1" value="276"/>
		<property name="com.jaspersoft.studio.data.sql.SQLQueryDesigner.sash.w2" value="718"/>
		<parameter name="idPermiso" class="java.math.BigDecimal"/>
		<parameter name="codigoIdioma" class="java.lang.String"/>
		<parameter name="codigoPais" class="java.lang.String"/>
		<queryString language="SQL">
			<![CDATA[select x.*, y.valor from (
select 1 as nivel, LABEL as label, parent_id as parent from FWK_ACTION_ITEM where ID = (select parent_id as parent from FWK_ACTION_ITEM where ID = (select parent_id as parent from FWK_ACTION_ITEM where ID_PERMISO = $P{idPermiso} and parent_id is not null) and parent_id is not null)
union
select case when (
select parent_id from FWK_ACTION_ITEM where ID = (select parent_id as parent from FWK_ACTION_ITEM where ID_PERMISO = $P{idPermiso} and parent_id is not null)
) is null then 1 else 2 end
as nivel, LABEL as label, parent_id as parent from FWK_ACTION_ITEM where ID = (select parent_id as parent from FWK_ACTION_ITEM where ID_PERMISO = $P{idPermiso} and parent_id is not null)
union
select case when (
select parent_id from FWK_ACTION_ITEM where ID = (select parent_id as parent from FWK_ACTION_ITEM where ID_PERMISO = $P{idPermiso} and parent_id is not null)
) is null then 2 else 3 end as nivel, LABEL as label, parent_id as parent from FWK_ACTION_ITEM where ID_PERMISO = $P{idPermiso} and parent_id is not null
union
(select a.* from (select 1 as nivel, i.label as label, i.parent_id as parent from FWK_ACTION_ITEM i where i.ID_PERMISO = $P{idPermiso} and i.parent_id is null) a 
join (
select 1 as nivel, LABEL as label, parent_id as parent from FWK_ACTION_ITEM where ID = (select parent_id as parent from FWK_ACTION_ITEM where ID = (select parent_id as parent from FWK_ACTION_ITEM where ID_PERMISO = $P{idPermiso} and parent_id is not null) and parent_id is not null)
union
select 2 as nivel, LABEL as label, parent_id as parent from FWK_ACTION_ITEM where ID = (select parent_id as parent from FWK_ACTION_ITEM where ID_PERMISO = $P{idPermiso} and parent_id is not null)
union
select 3 as nivel, LABEL as label, parent_id as parent from FWK_ACTION_ITEM where ID_PERMISO = $P{idPermiso} and parent_id is not null
) b  on a.label <> b.label and b.parent is null)
)x join FWK_CADENA_IDIOMA y on y.clave = x.label and y.id_idioma = (select id from FWK_IDIOMA where COD_IDIOMA = $P{codigoIdioma} and COD_PAIS = $P{codigoPais})]]>
		</queryString>
		<field name="nivel" class="java.lang.Integer"/>
		<field name="label" class="java.lang.String"/>
		<field name="parent" class="java.math.BigDecimal"/>
		<field name="valor" class="java.lang.String"/>
	</subDataset>
