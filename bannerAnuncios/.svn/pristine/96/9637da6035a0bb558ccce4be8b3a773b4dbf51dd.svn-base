		<queryString language="SQL">
			<![CDATA[select 
	spp.IdPerfil, spp.IdPermiso, 
	spe.Perfil, spe.Descripcion as descPerfil,
	sp.Permiso, sp.Descripcion as descPermiso
from SEG_PERMISO_PERFIL spp
join SEG_PERMISO sp on sp.Id = spp.IdPermiso
join SEG_PERFIL spe on spe.Id = spp.IdPerfil
order by spp.IdPerfil, sp.Permiso]]>
		</queryString>
