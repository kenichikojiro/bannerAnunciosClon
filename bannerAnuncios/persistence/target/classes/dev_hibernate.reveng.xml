<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<schema-selection match-catalog="BANNER" match-schema="dbo" />
	
	<table-filter match-catalog="BANNER" match-schema="dbo" match-name="DATABASECHANGE*" exclude="true"/>
	<table-filter match-catalog="BANNER" match-schema="dbo" match-name="FWK_*" exclude="true"/>
	<table-filter match-catalog="BANNER" match-schema="dbo" match-name="SEG_*" exclude="true"/>
	
	<table-filter match-catalog="BANNER" match-schema="dbo" match-name="BANNER_*" exclude="true"/>

	<!-- <table-filter match-catalog="BANNER" match-schema="dbo" match-name="BANNER_CC"/>-->
	
</hibernate-reverse-engineering>